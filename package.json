{
  "name": "llm-speed-test",
  "version": "1.0.0",
  "description": "A tool to test LLM inference speed",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "node-fetch": "^2.6.9"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}